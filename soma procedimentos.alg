algoritmo "semnome"

var
  X,Y: inteiro
  
Procedimento Soma (var A,B: inteiro)
Inicio
    A <- A + 1
    B <- B + 1
    Escreval("O valor de A é", A)
    Escreval("O valor de B é", B)
    Escreval("A soma A + B = ", A + B)
FimProcedimento
inicio
   X <- 4
   Y <- 8
   Soma (X, Y)
   Escreval("O valor de X = ", X)
   Escreval("O valor de Y = ", Y)
fimalgoritmo
