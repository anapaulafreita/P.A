algoritmo "semnome"

var
contador,valor,salto: inteiro
inicio
 escreval("Até que número quer contar?")
 leia (valor)
 escreval("Qual será o valor so salto?")
 leia (salto)
 contador <- 0
 enquanto (contador <= valor) faca
 escreval (contador)
    contador <- contador + salto
 FimEnquanto
 Escreval ("teminei de contar :)")
 
fimalgoritm0
